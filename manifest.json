{
    "manifest_version": 2,
    "name": "Rainbowify",
    "description": "Makes Headers turn into rainbows",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "include/jquery-2.1.4.min.js",
                "include/rainbow.js",
                "rainbowify.js"
            ],
            "run_at": "document_end"
        }
    ]
}
